                <th>
                    @Html.DisplayNameFor(model => model.AppointmentDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GivenPrice)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EstimatedPrice)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Not)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsRegister)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataQuality)
