                    <td>
                        @Html.DisplayFor(modelItem => item.GivenPrice)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EstimatedPrice)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Not)
